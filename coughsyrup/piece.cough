define ([
  'mydash'
  'constants'
  'constructors'
  'utilities'
  'extractors'
  'chessman'
  'assertions'
], (_, c, g, u, x, chessman-fns, v) ->

  setPassantPosition = ->
    c.unset-passant-pos

  setPromotionPosition = ->
    c.unset-promotion-pos

  public-fns = _.defaults [chessman-fns] {
    setPassantPosition
    setPromotionPosition
  }

  return public-fns

)
