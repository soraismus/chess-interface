define (['base-helpers', 'constants', 'chessman'], (_, c, chessman-fns) ->

  setPassantPosition = ->
    c.unset-passant-pos

  setPromotionPosition = ->
    c.unset-promotion-pos

  public-fns = _.defaults ({
    setPassantPosition
    setPromotionPosition
  }, chessman-fns)

  return public-fns

)
